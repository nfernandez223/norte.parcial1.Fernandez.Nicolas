<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - UAI</title>
    <link href="~/Content/Site.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
</head>
<body>
    <div id="container">
        <header>
            <article>
                <div>
                    <h1>¿Hay vida en otros planetas? - Sistemas solares</h1>
                </div>
            </article>

        </header>
        <section>
            <article>
                @RenderBody()
            </article>
        </section>

        <aside id="pub">
            <nav>
                <ul>
                    <li><a href="/">Contenido Relacionado</a> </li>
                    <li><a href="/Home/Contact">Contacto</a> </li>
                    <li>@Html.ActionLink("Admin Constelacion", "Cosmos", "Home")</li>

                </ul>
            </nav>

        </aside>
        <footer>
            <p>&copy; ¿Hay vida en otros planetas? @DateTime.Now.Year </p>
        </footer>
    </div>
    @RenderSection("scripts", required: false)
</body>
</html>